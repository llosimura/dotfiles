#!/bin/sh
#Generated by JetBrains Toolbox 1.20.7940 at 2021-04-13T16:07:24.761693

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/alfonso/Library/Application Support/JetBrains/Toolbox/apps/IDEA-U/ch-1/211.6693.111/IntelliJ IDEA 2021.1 EAP.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"